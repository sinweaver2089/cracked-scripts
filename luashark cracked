local Button328_call225 = Button328(Tab324_call221, {
    ["Name"] = "Instant Proximity Prompts",
    ["Callback"] = function()
        for _, v in ipairs(workspace:GetDescendants()) do
            if v:IsA("ProximityPrompt") then
                v.HoldDuration = 0
            end
        end
    end
})

local Section331 = Tab324_call221.CreateSection
local Section331_call228 = Section331(Tab324_call221, "Teleportations")

local function teleportToLocation(position)
    if LocalPlayer.Character then
        local HumanoidRootPart = LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
        if HumanoidRootPart then
            HumanoidRootPart.CFrame = CFrame.new(position)
        end
    end
end

