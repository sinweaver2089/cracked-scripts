    ["Callback"] = function()
        local Lighting = game:GetService("Lighting")
        if Lighting:FindFirstChild("deathblur") then
            Lighting.deathblur.Enabled = false
        end
        if Lighting:FindFirstChild("fireblur") then
            Lighting.fireblur.Enabled = false
        end
    end
})

