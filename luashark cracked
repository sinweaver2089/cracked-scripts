local Section165 = Tab146_call108.CreateSection
local Section165_call118 = Section165(Tab146_call108, "Movement Tools")

local Button166 = Tab146_call108.CreateButton
local Button166_call119 = Button166(Tab146_call108, {
    ["Name"] = "Teleport Tool",
    ["Callback"] = function()
        getgenv().speed = 30
        
        local TweenService = game:GetService("TweenService")
        local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
        local Mouse = LocalPlayer:GetMouse()
        
        local function teleportTo(position, speedValue)
            local HumanoidRootPart = Character:FindFirstChild("HumanoidRootPart")
            if HumanoidRootPart then
                local distance = (position.Position - HumanoidRootPart.Position).Magnitude
                local time = distance / speedValue
                
                local tweenInfo = TweenInfo.new(time, Enum.EasingStyle.Linear)
                local tween = TweenService:Create(HumanoidRootPart, tweenInfo, {CFrame = position})
                tween:Play()
                tween.Completed:Wait()
            end
        end
        
        local teleportTool = Instance.new("Tool")
        teleportTool.RequiresHandle = false
        teleportTool.Name = "Teleport Tool"
        
        teleportTool.Activated:Connect(function()
            local targetPosition = Mouse.Hit + Vector3.new(0, 2.5, 0)
            teleportTo(CFrame.new(targetPosition.X, targetPosition.Y, targetPosition.Z), getgenv().speed)
        end)
        
        teleportTool.Parent = LocalPlayer.Backpack
    end
})

