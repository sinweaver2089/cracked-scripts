local Section266 = Tab205_call137.CreateSection
local Section266_call180 = Section266(Tab205_call137, "Visual Combat Scripts")

local espEnabled = false
local espConnections = {}

local Toggle269 = Tab205_call137.CreateToggle
local Toggle269_call183 = Toggle269(Tab205_call137, {
    ["CurrentValue"] = false,
    ["Callback"] = function(Value)
        espEnabled = Value
        
        if Value then
            -- Create ESP for existing players
            for _, player in ipairs(Players:GetPlayers()) do
                if player ~= LocalPlayer then
                    createESP(player)
                end
            end
            
            -- Connect to player added event
            table.insert(espConnections, Players.PlayerAdded:Connect(function(player)
                createESP(player)
            end))
        else
            -- Clean up ESP
            for _, connection in ipairs(espConnections) do
                connection:Disconnect()
            end
            espConnections = {}
            
            for _, player in ipairs(Players:GetPlayers()) do
                if player ~= LocalPlayer and player.Character then
                    local head = player.Character:FindFirstChild("Head")
                    if head then
                        local espTag = head:FindFirstChild("PlayerNametagESP")
                        if espTag then
                            espTag:Destroy()
                        end
                    end
                end
            end
        end
    end,
    ["Name"] = "Player Nametag ESP",
    ["Flag"] = "PlayerNametagESP"
})

local function createESP(player)
    if not espEnabled then return end
    
    local function addESP(character)
        task.wait(1)
        if not character then return end
        
        local head = character:FindFirstChild("Head")
        if not head then return end
        
        local existingTag = head:FindFirstChild("PlayerNametagESP")
        if existingTag then existingTag:Destroy() end
        
        local billboard = Instance.new("BillboardGui")
        billboard.Name = "PlayerNametagESP"
        billboard.Adornee = head
        billboard.Size = UDim2.new(0, 100, 0, 40)
        billboard.StudsOffset = Vector3.new(0, 3, 0)
        billboard.AlwaysOnTop = true
        
        local textLabel = Instance.new("TextLabel")
        textLabel.Size = UDim2.new(1, 0, 1, 0)
        textLabel.BackgroundTransparency = 1
        textLabel.Text = player.Name
        textLabel.TextColor3 = Color3.new(1, 1, 1)
        textLabel.TextScaled = true
        textLabel.Font = Enum.Font.SourceSansBold
        textLabel.Parent = billboard
        
        billboard.Parent = head
    end
    
    if player.Character then
        addESP(player.Character)
    end
    
    table.insert(espConnections, player.CharacterAdded:Connect(addESP))
end

